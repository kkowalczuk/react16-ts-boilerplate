{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-conditional-assignment": true,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "double"
    ],
    "ordered-imports": [
      true,
      {
        "import-sources-order": "case-insensitive",
        "grouped-imports": true,
        "named-imports-order": "case-insensitive"
      }
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      true,
      "no-public"
    ]
  }
}