{
  "extends": [
    "tslint:recommended",
    "tslint:react"
  ],
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-conditional-assignment": true,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "double"
    ]
  }
}